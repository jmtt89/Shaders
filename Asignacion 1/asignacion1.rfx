     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
//Variables de Fragment Program
uniform float Il,Ia,Ie,shine,roughness,sharpness;


//variables para comunicarse con el vertex program
varying vec4 vPos,vMD,vMA,vMS,vCL;
varying vec3 vN,vld;


void main(void)
{
   vec3 r,H;
   vec4 cAmb,cDiff,cSpec,colorFinal;
   float dotNL,w;
   cDiff = vec4(0.0,0.0,0.0,0.0);
   cSpec = vec4(0.0,0.0,0.0,0.0);
   

   //Producto punto de la normal por la luz
   dotNL = max(0.0,dot(normalize(vN),normalize(vld.xyz)));

   //Componente Ambiental
   cAmb = Ia * vMA;
   
   
   //Componente Difusa
   cDiff = Il * vCL * vMD * dotNL;
   
   //Componente Especular
   if(dotNL>0.0){
      w = 0.18 * (1.0 - sharpness);
      r = normalize(reflect(-normalize(vld.xyz),vN));
      cSpec = Ie * smoothstep(0.72 - w, 0.72 + w, pow(max(0.0,dot(r,-normalize(vPos.xyz))),shine)) * vMS;
   }
   
   //Formula Final
   colorFinal = cAmb + cDiff + cSpec ; //Interpolacion del color
   
   gl_FragColor = colorFinal;
}]]></RmShaderCode></RmGLShader>
