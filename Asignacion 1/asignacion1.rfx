     <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform vec4 ligthDirection, cMA, cMD, cL, cMS;

//cMA Color Ambiental
//cMD Color del Modelo
//cMS Color Especular
//cL  Color de la Luz

varying vec4 vPos,vMD,vMA,vMS,vCL;
varying vec3 vN,vld;

void main(void)
{

   vec4 ld;
    
   vN = normalize(gl_NormalMatrix * gl_Normal); //vector Normal al Vertice
   ld = gl_ModelViewMatrix * ligthDirection; // luz direccional en coordenadas de la camara
   
   vPos = gl_ModelViewMatrix * gl_Vertex; //Posicion de la Camarara, en coordenadas desde la camara
   
   //El 1 representa que la luz es direcional, diferente a 1 es direccional y con respecto a eso se asigna el vld al fragment
   if(int(ld.w) == 1)
      vld = normalize(ld.xyz - vPos.xyz);
   else
      vld = normalize(ld.xyz);

   vMA = cMA;
   vMD = cMD;
   vMS = cMS;
   vCL = cL;

   gl_Position = ftransform();
}]]></RmShaderCode></RmGLShader>
     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
//Variables de Fragment Program
uniform float Il,Ia,Ie,shine,wrap,gam;


//variables para comunicarse con el vertex program
varying vec4 vPos,vMD,vMA,vMS,vCL;
varying vec3 vN,vld;


void main(void)
{
   vec3 r;
   vec4 cAmb,cDiff,cSpec,colorFinal;
   float dotNL,dotP,wrp,wa,diffuze; 
   cSpec = vec4(0.0,0.0,0.0,0.0);



   //Producto punto de la normal por la luz
   dotNL = max(0.0,dot(normalize(vN),normalize(vld.xyz)));

   //Componente Ambiental
   cAmb = Ia * vMA;
   
   
   //Componente Difusa
   wrp = 1.0-0.5*wrap;
   wa = acos(wrp*2.0-1.0);
   dotP = 0.5*(1.0+dotNL);
   if(dotP <= wrp){
      diffuze = pow((wrp-dotP)/(wrp),gam);
   } else {
      diffuze = pow((dotP-wrp)/(1.0-wrp),gam);
   }
   
   cDiff = Il * vCL * vMD * diffuze;
   
   //Componente Especular
   if(dotNL>0.0){
      r = normalize(reflect(-normalize(vld.xyz),vN));
      cSpec = pow(max(0.0,dot(r,-normalize(vPos.xyz))),shine) * vMS;
      cSpec *= Ie;
   }
   
   //Formula Final
   colorFinal = cAmb + cDiff+ cSpec; //Interpolacion del color
   
   gl_FragColor = colorFinal;
}]]></RmShaderCode></RmGLShader>
