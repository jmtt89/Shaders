     <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform vec4 lightDirection, cMA, cMD, cL, cMS;
uniform float intL, shine;
uniform bool useBump;
//varibles Stream
attribute vec3 rm_tangent;
attribute vec3 rm_binormal;

varying vec4 vPos, vMD, vMA, vMS;
varying vec3 vN, vld, vC;

varying vec2 texcoord;

void main()
{
      //opengl me da la normal, tengo que transformar todo
   // a un espacio de la camara, ya que la gl_normal esta
   // en las coordenadas mundo
   vec4 ld;
   vec3 ldAux;
   
   vN = normalize(gl_NormalMatrix * gl_Normal);   
   ld = gl_ModelViewMatrix * lightDirection;

   //Recuperar UV
   texcoord = gl_MultiTexCoord0.st;
      
   //Componente Especular
   vPos = gl_ModelViewMatrix * gl_Vertex;
   
   
   if (int(ld.w) == 1)
      ldAux = normalize(ld.xyz - vPos.xyz);
   else
      ldAux = normalize(ld.xyz);
   
   //Espacio tangente
   
   vec3 cam  = -vPos.xyz;
   vec3 t = gl_NormalMatrix*rm_tangent;
   vec3 b = gl_NormalMatrix*rm_binormal;
   
   if(useBump){
      vC.x = dot(t,cam);
      vC.y = dot(b,cam);
      vC.z = dot(vN,cam);
      vld.x = dot(t,ldAux);
      vld.y = dot(b, ldAux);
      vld.z = dot(vN, ldAux);
   }else{
      vC = cam;
      vld = ldAux;
   }
   
   vMD = cMD;
   vMA = cMA;
   vMS = cMS;

   gl_Position = ftransform();
}]]></RmShaderCode></RmGLShader>
     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
//shine que tan grande es el brillo
uniform bool useTexture;
uniform float intL, shine, repeatU, repeatV, rotText, bumpValue;
uniform bool useBump;

varying vec4 vPos, vMD, vMA, vMS;
varying vec3 vN, vld, vC;

uniform sampler2D txColor01;
uniform sampler2D txBump;
varying vec2 texcoord;

void main()
{   
   vec3 r;
   vec4  Kdiff, cSpec;
   float dotNL;
   
   //Rotacion UV
   vec2 auxst, rst;
   auxst.s = texcoord.s - 0.5;
   auxst.t = texcoord.t - 0.5;
   
   rst.s = cos(rotText)*auxst.s - sin(rotText)*auxst.t;
   rst.t = sin(rotText)*auxst.s + cos(rotText)*auxst.t;
   
   auxst.s = rst.s + 0.5;
   auxst.t = rst.t + 0.5;
   
   //Repetir UV
   vec2 ftexcoord;
   
   ftexcoord.s = repeatU*auxst.s;
   ftexcoord.t = repeatV*auxst.t;
   
   
   //Recuperar Color Textura
   vec4 cColorT = texture2D(txColor01, ftexcoord);
   vec4 cBumpT = texture2D(txBump,ftexcoord);
   ivec2 wh = textureSize2D(txBump,0);
   float fracX = 1.0/float(wh.x);
   float fracY = 1.0/float(wh.y);
   vec4 H1 = texture2D(txBump,vec2(ftexcoord.s + fracX,ftexcoord.t));
   vec4 H2 = texture2D(txBump,vec2(ftexcoord.s - fracX,ftexcoord.t));
   vec4 H3 = texture2D(txBump,vec2(ftexcoord.s,ftexcoord.t + fracY));
   vec4 H4 = texture2D(txBump,vec2(ftexcoord.s,ftexcoord.t - fracY));
   
   vec3 t = vec3(1.0, 0.0, bumpValue*(H1.xyz-H2.xyz));
   vec3 b = vec3(0.0,1.0,bumpValue*(H3.xyz-H4.xyz));
   vec3 bn = cross(t,b);
   
   vec3 N;
   if(useBump){
      N = normalize(bn);
   }else{
      N = normalize(vN);
   }
//Componente difusa
   dotNL = max(0.0 , dot( N , normalize(vld.xyz) ) );
   
   vec4 cAmbiental;
   
   if (useTexture) {
      Kdiff = dotNL * vMD * cColorT;
      cAmbiental = vMA * cColorT;
   }else{
      Kdiff = dotNL * vMD;
      cAmbiental = vMA;
   }
   
   
   cSpec = vec4(0.0,0.0,0.0,0.0);
   if(dotNL > 0.0){
   // El negativo es por que opengl toma ese vector de la manera contraria
   // Direccion opueta, por eso el negativo
      r = normalize(reflect(-normalize(vld.xyz),N));
      cSpec = pow( max(dot(r,-(normalize(vPos.xyz)) ),0.0),shine) * vMS;
   }   
   
   vec4 colorFinal;
   
   colorFinal = cAmbiental + intL*(Kdiff + cSpec);
   
   gl_FragColor =colorFinal;
}]]></RmShaderCode></RmGLShader>
