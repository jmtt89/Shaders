     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
//shine que tan grande es el brillo
uniform bool useTexture;
uniform float intL, shine, repeatU, repeatV, rotText, wTile, hTile;
uniform vec4 cTile, cBack;

varying vec4 vPos, vMD, vMA, vMS;
varying vec3 vN, vld;

uniform sampler2D txColor01;
varying vec2 texcoord;

void main()
{   
   vec3 r;
   vec4  Kdiff, cSpec;
   float dotNL;
   
   //Rotacion UV
   vec2 auxst, rst;
   auxst.s = texcoord.s - 0.5;
   auxst.t = texcoord.t - 0.5;
   
   rst.s = cos(rotText)*auxst.s - sin(rotText)*auxst.t;
   rst.t = sin(rotText)*auxst.s + cos(rotText)*auxst.t;
   
   auxst.s = rst.s + 0.5;
   auxst.t = rst.t + 0.5;
   
   //Repetir UV
   vec2 ftexcoord;
   
   ftexcoord.s = mod(repeatU*auxst.s,1.0);
   ftexcoord.t = mod(repeatV*auxst.t,1.0);
   
   
   //Recuperar Color Textura
   vec4 cColorT;
   float row, col;
   
   row = step(wTile/2.0,ftexcoord.s) * (1.0-step(1.0-(wTile/2.0),ftexcoord.s));
   col = step(hTile/2.0,ftexcoord.t) * (1.0-step(1.0-(hTile/2.0),ftexcoord.t));
   
   cColorT = cTile*(row*col) + cBack*(1.0 - (row*col));
   
   
//Componente difusa
   dotNL = max(0.0 , dot( normalize(vN) , normalize(vld.xyz) ) );
   
   vec4 cAmbiental;
   
   if (useTexture) {
      Kdiff = dotNL * vMD * cColorT;
      cAmbiental = vMA * cColorT;
   }else{
      Kdiff = dotNL * vMD;
      cAmbiental = vMA;
   }
   
   
   cSpec = vec4(0.0,0.0,0.0,0.0);
   if(dotNL > 0.0){
   // El negativo es por que opengl toma ese vector de la manera contraria
   // Direccion opueta, por eso el negativo
      r = normalize(reflect(-normalize(vld.xyz),vN));
      cSpec = pow( max(dot(r,-(normalize(vPos.xyz)) ),0.0),shine) * vMS;
   }   
   
   vec4 colorFinal;
   
   colorFinal = cAmbiental + intL*(Kdiff + cSpec);
   
   gl_FragColor =colorFinal;
}]]></RmShaderCode></RmGLShader>
